strict graph "" {
	a	 [color=red,
		style="selected vertex"];
	b	 [color=red,
		style="selected vertex"];
	a -- b	 [color=black,
		style="edge"];
	c	 [style=vertex];
	a -- c	 [color=black,
		style="edge"];
	d	 [style=vertex];
	a -- d	 [color=black,
		style="edge"];
	e	 [color=red,
		style="selected vertex"];
	a -- e	 [color=red,
		style="edge"];
	b -- c	 [color=black,
		style="edge"];
	b -- e	 [color=red,
		style="edge"];
}
